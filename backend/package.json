{
  "name": "src",
  "version": "0.0.0",
  "engines": {
    "node": "10.16.0",
    "npm": "5.0.3"
  },
  "private": true,
  "scripts": {
    "start": "env NODE_ENV=heroku node ./src/bin/www",
    "prod": "env NODE_ENV=production DATABASE_URL=postgres://postgres:postgres@localhost:5432/recapp node ./src/bin/www",
    "dev": "env NODE_ENV=development DATABASE_URL=postgres://postgres:postgres@localhost:5432/recapp nodemon ./src/bin/www",
    "docker": "nodemon ./src/bin/www",
    "lint": "eslint . --fix",
    "test": "env NODE_ENV=development env DATABASE_URL=postgres://postgres:postgres@localhost:5432/recapp ./node_modules/.bin/jest",
    "test-docker": "env NODE_ENV=development env DATABASE_URL=postgres://postgres:postgres@dev_db:5432/recapp ./node_modules/.bin/jest"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^0.19.1",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "node-pre-gyp": "^0.14.0",
    "path": "^0.12.7",
    "pg": "^7.17.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2"
  }
}
